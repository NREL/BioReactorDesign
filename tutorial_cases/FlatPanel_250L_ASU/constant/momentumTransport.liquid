FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      momentumTransport.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType  RAS;

RAS
{
    model    mixtureKEpsilon; // LaheyKEpsilon;

    turbulence      on;
    printCoeffs     on;
}

