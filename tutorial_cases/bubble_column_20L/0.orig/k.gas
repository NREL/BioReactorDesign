FoamFile
{
    format      ascii;
    class       volScalarField;
    object      k.gas;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

#include "${FOAM_CASE}/constant/globalVars"

internalField   uniform $k_inlet_gas;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type           fixedValue;
        value          uniform $k_inlet_gas;
    }

    outlet
    {
        type            zeroGradient;
    }

    defaultFaces
    {
        type            zeroGradient;
    }
}

