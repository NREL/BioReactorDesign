FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermophysicalProperties.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "$FOAM_CASE/constant/globalVars"

thermoType
{
    type            heRhoThermo;
    mixture         multiComponentMixture;
    transport       const;
    thermo          hConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleInternalEnergy;
}

species
(
    H2O
    O2
);

defaultSpecie H2O;

"(mixture|H2O)"
{
    specie
    {
        nMoles      1;
        molWeight   18;
    }
    equationOfState
    {
        rho        $rho0MixLiq;
    }
    thermodynamics
    {
        Hf          -15857343;
        Cp          $CpMixLiq;
    }
    transport
    {
        mu          $muMixLiq;
        Pr          $PrMixLiq;
    }
}

