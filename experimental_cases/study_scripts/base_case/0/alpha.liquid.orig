FoamFile
{
    format      ascii;
    class       volScalarField;
    object      alpha.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 1;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type            fixedValue;
        value           uniform 0.5;
    }
    outlet
    {
        type            inletOutlet;
        phi             phi.liquid;
        inletValue      uniform 0;
        value           uniform 0;
    }
    "vessel|shaft|walls|impeller"
    {
    	// NOTE: zeroGradient corresponds to glass, while 60 might be plastic
    	type            zeroGradient; //contactAngle;
	theta0		60;
	limit		gradient;
	value		uniform 0;
    }
    
    nonCouple
    {
        type            zeroGradient;
    }

}

// ************************************************************************* //
