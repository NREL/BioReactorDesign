FoamFile
{
    format      ascii;
    class       surfaceScalarField;
    location    "0";
    object      meshPhi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
    propeller
    {
        type            calculated;
        value           uniform 0;
    }
    walls
    {
        type            calculated;
        value           uniform 0;
    }
    inside_to_hub
    {
        type            empty;
        value           nonuniform List<scalar> 0();
    }
    inside_to_hub_copy
    {
        type            empty;
        value           nonuniform List<scalar> 0();
    }
    hub_to_rotor
    {
        type            empty;
        value           nonuniform List<scalar> 0();
    }
    hub_to_rotor_copy
    {
        type            empty;
        value           nonuniform List<scalar> 0();
    }
}


