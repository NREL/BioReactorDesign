FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.0001;

boundaryField
{
    outlet
    {
        type            calculated;
        value           uniform 0.0001;
    }
    propeller
    {
        type            nutkWallFunction;
        value           uniform 0.0001;
    }
    walls
    {
        type            nutkWallFunction;
        value           uniform 0.0001;
    }
    inside_to_hub
    {
        type            empty;
    }
    inside_to_hub_copy
    {
        type            empty;
    }
    hub_to_rotor
    {
        type            empty;
    }
    hub_to_rotor_copy
    {
        type            empty;
    }
}


