FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 86126.2;

boundaryField
{
    outlet
    {
        type            prghPressure;
        p               uniform 86126.2;
        value           uniform 86126.2;
        rho             rho;
    }
    propeller
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 86126.2;
    }
    walls
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 86126.2;
    }
    inside_to_hub
    {
        type            empty;
    }
    inside_to_hub_copy
    {
        type            empty;
    }
    hub_to_rotor
    {
        type            empty;
    }
    hub_to_rotor_copy
    {
        type            empty;
    }
}


