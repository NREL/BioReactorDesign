FoamFile
{
    format      ascii;
    class       volScalarField;
    object      YDefault;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    outlet
    {
        type            inletOutlet;
        phi             phi.gas;
        inletValue      $internalField;
        value           $internalField;
    }

    "vessel|shaft|walls|impeller.*"
    {
        type            zeroGradient;
    }

    defaultFaces
    {
        type            empty;
    }
    
    nonCouple
    {
        type            zeroGradient;
    }

}

