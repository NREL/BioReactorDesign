FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    alpha.gas 1;
    alpha.liquid 0;
}

zones
{
    liquid
    {
        type box;
        zoneType cell;
        box (-1 -1 -1) (1 0.089 1);
        values
        {
            alpha.gas 0;
            alpha.liquid 1;
        };
    }
}


// ************************************************************************* //

