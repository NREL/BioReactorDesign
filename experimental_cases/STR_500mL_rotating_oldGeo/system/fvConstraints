FoamFile
{
    format      ascii;
    class       dictionary;
    object      fvConstraints;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

limitp
{
    type       limitPressure;

    min        1e4;
}

limitT
{
    type            limitTemperature;

    select          all;

    field           T;   // Optional energy/temperature field name
                            // Set to T for compressibleVoF

    phase           gas; // Optional phase name

    min             283;
    max             303;
}


