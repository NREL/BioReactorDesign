FoamFile
{
    format      ascii;
    class       volScalarField;
    object      H2.gas;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
dimensions      [0 0 0 0 0 0 0];

#include "${FOAM_CASE}/constant/globalVars"

internalField   uniform 0;


boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type            fixedValue;
        value           uniform $f_H2;
    }

    outlet
    {
        //type            inletOutlet;
        //phi             phi.gas;
        //inletValue      $f_H2;
        //value           $f_H2;
        type            zeroGradient;
    }

    defaultFaces
    {
        type            zeroGradient;
    }
}

