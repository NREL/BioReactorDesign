FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 7e-4;

boundaryField
{
    cyl2
    {
        type            fixedValue;
        value           $internalField;
    }
    cyl4
    {
        type            fixedValue;
        value           $internalField;
    }
    cyl1
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
    cyl3
    {
        type            epsilonWallFunction;
        value           $internalField;
    }


    outlet
    {
        type            inletOutlet;
        phi             phi.liquid;
        inletValue      $internalField;
        value           $internalField;
    }

    walls
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
    pipes
    {
        type            epsilonWallFunction;
        value           $internalField;
    }

    defaultFaces
    {
        type            empty;
    }
}

