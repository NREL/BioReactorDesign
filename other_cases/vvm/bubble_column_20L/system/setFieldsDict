FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.gas 0.99
    volScalarFieldValue alpha.liquid 0.01
    volVectorFieldValue U.gas (0 0 0)
);

regions
(
    boxToCell
    {
        box (-1.0 -1.0 -1.0) (10 1.34016128 10);
        fieldValues
        (
            volScalarFieldValue alpha.gas 0.01
            volScalarFieldValue alpha.liquid 0.99
            volVectorFieldValue U.gas (0 0 0)
        );
    }
);


